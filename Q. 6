#include <iostream>
#include <cstdio>
using namespace std;

/*
Add `int max_of_four(int a, int b, int c, int d)` here.
*/

int main() {
    int a;
    int b;
    int c;
    int d;
    cin>>a>>b>>c>>d;
    int s=max(max(a,b),max(c,d));
    cout<<s;
    
    return 0;
}
